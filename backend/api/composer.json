{
    "name": "afagorn/satushem",
    "type": "project",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/orm": "^2.10",
        "laminas/laminas-config-aggregator": "^1.7",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.2",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "symfony/cache": "^6.0",
        "symfony/console": "^5.4",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
